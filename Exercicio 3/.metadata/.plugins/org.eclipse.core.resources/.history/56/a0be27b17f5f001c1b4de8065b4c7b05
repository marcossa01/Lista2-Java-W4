import java.util.Arrays;

public class RadixSort {
	
	public static void radixSort (int iArr[]) {
		
	
		
		
	}
	
	public static void main(String[] args) {
		
		int numerosCasa = 5;
		
		int iArr[] = {16223, 898, 13, 906, 235, 23, 9, 1532, 6388, 2501, 8};
		
		String array = Arrays.toString(iArr);
		String ar[] = array.substring(1, array.length()-1).split(", ");
		
		for(int i = 0; i < ar.length; i++) {
			String s = String.format(ar[i], numerosCasa);
			System.out.println(s);
		}
		
		System.out.println();
		
		radixSort(iArr);
		
		for (int i : iArr) {
			System.out.println(i + " ");
		}
		
	}

}
